	<queryString>
		<![CDATA[SELECT alquilermodem.codigoalquiler,fechainicioalquiler,alquilermodem.codigomodem,nombremodem,
alquilermodem.cedulacliente,nombrecliente,direccioncliente,telefonocliente,correocliente,
multa,(alquilermodem.preciodia*(fechafinalquiler-fechainicioalquiler)) as dias 
FROM alquilermodem inner join cliente on alquilermodem.cedulacliente=cliente.cedulacliente 
inner join usbmodem on alquilermodem.codigomodem=usbmodem.codigomodem
where alquilermodem.cedulacliente=$P{cedulaC} and fechadevolucion!='1899-12-31'
order by codigomodem]]>
	</queryString>
