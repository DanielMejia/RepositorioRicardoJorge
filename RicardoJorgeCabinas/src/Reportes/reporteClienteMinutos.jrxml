	<queryString>
		<![CDATA[SELECT ventaminutos.codigoventa,fechaventa,ventaminutos.codigoplan,nombreplan,
ventaminutos.cedulacliente,nombrecliente,direccioncliente,telefonocliente,correocliente,
minutosvendidos,minutosfacturados,(ventaminutos.preciominuto*minutosfacturados) as totalventa 
FROM ventaminutos inner join cliente on ventaminutos.cedulacliente=cliente.cedulacliente 
inner join planminutos on ventaminutos.codigoplan=planminutos.codigoplan 
where ventaminutos.cedulacliente=$P{cedulaC}
order by codigoplan,fechaventa]]>
	</queryString>
