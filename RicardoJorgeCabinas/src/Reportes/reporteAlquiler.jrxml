	<queryString>
		<![CDATA[select 
codigoalquiler,fechainicioalquiler,fechafinalquiler,multa,nombreusuario,nombrecliente,nombremodem,(alquilermodem.preciodia*(fechafinalquiler-fechainicioalquiler)) as total

from alquilermodem
inner join cliente on alquilermodem.cedulacliente=cliente.cedulacliente 
inner join usuario on  alquilermodem.cedulausuario=usuario.cedulausuario
inner join usbmodem on  alquilermodem.codigomodem=usbmodem.codigomodem


where fechainicioalquiler >= $P{fechaInicio} and fechainicioalquiler <= $P{fechaFin}]]>
	</queryString>
