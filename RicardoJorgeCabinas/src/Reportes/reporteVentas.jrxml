	<queryString>
		<![CDATA[select codigoventa,fechaventa,minutosvendidos,minutosfacturados,nombrecliente,nombreplan,nombreusuario,(ventaminutos.preciominuto*minutosfacturados) as total
from ventaminutos 
inner join cliente on ventaminutos.cedulacliente=cliente.cedulacliente 
inner join usuario on  ventaminutos.cedulausuario=usuario.cedulausuario
inner join planminutos on  ventaminutos.codigoplan=planminutos.codigoplan


where fechaventa >= $P{fechaInicio} and fechaventa <= $P{fechaFin}]]>
	</queryString>
